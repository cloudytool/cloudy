{
  "name": "@cloudytool/cloudy",
  "private": false,
  "version": "0.0.3",
  "description": "Cloudy is an \"infrastructure as code\" tool for managing production-grade cloud clusters.",
  "author": "Ivan Fokeev @ifokeev",
  "bin": {
    "cloudy": "./bin/run"
  },
  "homepage": "https://github.com/cloudytool/cloudy",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "cloudytool/cloudy",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-update": "^3.0.0",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "cli-ux": "^6.0.9",
    "fs-extra": "^10.1.0",
    "git-clone": "^0.2.0",
    "which": "^2.0.2",
    "write-yaml-file": "^4.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/git-clone": "^0.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.56",
    "@types/which": "^2.0.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "cloudy",
    "dirname": "cloudy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.cloudy.cli"
    },
    "update": {
      "s3": {
        "bucket": "cloudy-releases"
      }
    },
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:show-rules": "eslint --print-config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "push": "yarn publish --access public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/cloudytool/cloudy/issues",
  "keywords": [
    "oclif",
    "pulumi",
    "cloud",
    "infrastructure",
    "as code",
    "cli",
    "cloudy",
    "cloudy lab"
  ],
  "types": "dist/index.d.ts"
}
